{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "import/extensions": ["error", "always"],
    "camelcase": "off",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "max-len": ["error", { "code": 100 }],
    "consistent-return": "off",
    "no-await-in-loop": "off",
    "no-restricted-syntax": "off",
    "object-curly-newline": "off"
  }
}